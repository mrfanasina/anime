@router.post("/anime/update-info/{anime_id}")
def update_anime_info(anime_id: int):
    """
    Met  jour les informations d'un anim spcifique depuis MyAnimeList.
    (Score, image, description, statut, type, rank, etc.)
    """
    db = SessionLocal()
    try:
        anime = db.query(Anime).filter_by(id=anime_id).first()
        if not anime:
            db.close()
            return {"error": "Anim introuvable"}
        
        logging.info(f"[UPDATE-INFO] Rcupration des infos pour {anime.name}")
        info = get_anime_info(anime.name)
        if not info:
            db.close()
            return {"error": f"Aucune information trouve pour {anime.name}"}

        update_anime_info_in_db(db, anime.id, info)
        db.refresh(anime)

        return {
            "message": f"Informations mises  jour pour {anime.name} ",
            "anime": {
                "id": anime.id,
                "name": anime.name,
                "score": info.get("score"),
                "rank": info.get("rank"),
                "status": info.get("status"),
                "type": info.get("type"),
                "description": info.get("description"),
                "image_url": info.get("image_url")
            }
        }
    except Exception as e:
        logging.error(f"[UPDATE-INFO] Erreur: {e}")
        return {"error": str(e)}
    finally:
        db.close()

