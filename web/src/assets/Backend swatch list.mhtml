@router.get("/anime/update-info/all")
def update_all_anime_info():
    """
    Met  jour les informations de tous les anims depuis MyAnimeList.
    (Score, image, description, statut, type, rank, etc.)
    """
    db = SessionLocal()
    try:
        animes = db.query(Anime).all()
        for anime in animes:
            logging.info(f"[UPDATE-INFO-ALL] Rcupration des infos pour {anime.name}")
            info = get_anime_info(anime.name)
            if info:
                update_anime_info_in_db(db, anime.id, info)
                logging.info(f"[UPDATE-INFO-ALL] Infos mises  jour pour {anime.name}")
            else:
                logging.info(f"[UPDATE-INFO-ALL] Aucune info trouve pour {anime.name}")
        return {"message": "Mise  jour des informations termine"}
    except Exception as e:
        logging.error(f"[UPDATE-INFO-ALL] Erreur: {e}")
        return {"error": str(e)}
    finally:
        db.close()

