# ---------------------------
# * Gestion du classement ELO
# ---------------------------
def expected_score(rating_a, rating_b):
    return 1 / (1 + 10 ** ((rating_b - rating_a) / 400))

def update_elo(anime1, anime2, winner, k=3D32):
    R1, R2 =3D anime1.elo, anime2.elo
    E1, E2 =3D expected_score(R1, R2), expected_score(R2, R1)
    S1, S2 =3D (1, 0) if winner =3D=3D anime1.id else (0, 1)
    anime1.elo =3D round(R1 + k * (S1 - E1))
    anime2.elo =3D round(R2 + k * (S2 - E2))

@router.post("/duel")
def duel(anime1_id: int =3D Body(...), anime2_id: int =3D Body(...), winner=
_id: int =3D Body(...)):
    db =3D SessionLocal()
    anime1 =3D db.query(Anime).filter_by(id=3Danime1_id).first()
    anime2 =3D db.query(Anime).filter_by(id=3Danime2_id).first()
    if not anime1 or not anime2 or winner_id not in [anime1_id, anime2_id]:
        db.close()
        return {"error": "Anim=C3=A9s invalides"}
    update_elo(anime1, anime2, winner=3Dwinner_id)
    db.commit()
    classement =3D db.query(Anime).order_by(Anime.elo.desc()).limit(10).all=
()
    db.close()
    return {
        "message": "Elo mis =C3=A0 jour",
        "classement": [
            {"id": a.id, "name": a.name, "elo": a.elo} for a in classement
        ]
    }


